<?xml version="1.0" encoding="iso-8859-1" ?>
  
<movie version="9" width="600" height="300" framerate="24">
  <background color="#ffffff"/>
  <frame>

    <!-- non-library items, used below  -->
    <clip id="CLP_Peg_Blue" import="img/peg_blue.png"  />
    <clip id="CLP_Peg_Red" import="img/peg_red.png"  />
    <clip id="CLP_Peg_Gold" import="img/peg_gold.png"  />
    <clip id="CLP_Peg_Blue_Lit" import="img/peg_blue_lit.png"  />
    <clip id="CLP_Peg_Red_Lit" import="img/peg_red_lit.png"  />
    <clip id="CLP_Peg_Gold_Lit" import="img/peg_gold_lit.png"  />

    <!-- library items will have asset classes generated for them  -->
    <library>
      <bitmap id="BMP_Ball" import="img/ball.png"  />
      <bitmap id="BMP_BonusChuteGraphic" import="img/bonus_chute.png" />
      <bitmap id="BMP_Shooter" import="img/shooter.png" />


      <clip id="PegMovie">
        <frame>
          <place id="CLP_Peg_Blue" depth="1" x="-12" y="-12" />
          <stop />
        </frame>
        <frame>
          <place id="CLP_Peg_Blue_Lit" depth="2" x="-12" y="-12"/>
          <stop />
        </frame>
        <frame>
          <place id="CLP_Peg_Red" depth="3" x="-12" y="-12"/>
          <stop />
        </frame>
        <frame>
          <place id="CLP_Peg_Red_Lit" depth="4" x="-12" y="-12"/>
          <stop />
        </frame>
        <frame>
          <place id="CLP_Peg_Gold" depth="5" x="-12" y="-12"/>
          <stop />
        </frame>
        <frame>
          <place id="CLP_Peg_Gold_Lit" depth="6" x="-12" y="-12"/>
          <stop />
        </frame>
      </clip>

      <font id="vera" name="vera" import="font/Vera.ttf" />
      <font id="blox2" name="blox2" import="font/Blox2.ttf" />
    </library>
  </frame>
</movie>
